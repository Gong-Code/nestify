{
    "version": 2,
    "builds": [
      {
        "src": "/nextify/app/package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/nextify/app/$1"
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/nextify/app"
      }
    ]
  }